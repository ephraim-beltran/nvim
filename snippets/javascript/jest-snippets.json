{
  "Describe block": {
    "description": "Describe block for js testing",
    "prefix": "desc",
    "body": [
      "describe(\"${1:Function to test}\", () => {",
      "\t${2: // Test here}",
      "});"
    ]
  },
  "Testing block": {
    "description": "Testing block for js testing",
    "prefix": "it",
    "body": [
      "it(\"${1:Function to test}\", () => {",
      "\t${2: // Test here}",
      "});"
    ]
  },
  "Async Testing block": {
    "prefix": "ita",
    "description": "Asynchronous testing block for js testing",
    "body": [
      "it(\"${1:Function to test}\", async () => {",
      "\t${2: // Test here}",
      "});"
    ]
  },
  "Render component": {
    "description": "Invoke testing-library `render` function",
    "prefix": "ren",
    "body": [
      "render(${1:<${2:Component}} />);"
    ]
  },
  "Testing Todo": {
    "description": "Testing blocks that need to be done",
    "prefix": "itd",
    "body": [
      "it.todo(\"${1:Unimplemented Test}\");"
    ]
  }
}
